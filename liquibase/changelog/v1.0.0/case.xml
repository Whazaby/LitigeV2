<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

	<changeSet author="Guillaume" id="case-1">
		<createSequence cycle="true" incrementBy="1" schemaName="public" sequenceName="seq_case_id"
			startValue="1" />
	</changeSet>
	<changeSet author="Guillaume" id="case-2">
		<createTable tableName="case">
			<column name="id" type="numeric(19)" defaultValueSequenceNext="seq_case_id">
				<constraints nullable="false" />
			</column>
			<column name="creation" type="TIMESTAMP(29) WITH TIME ZONE" />
			<column name="litige" type="numeric(19)" />
			<column name="agent" type="numeric(19)" />
			<column name="resolution" type="VARCHAR(200)" />
		</createTable>
		<addForeignKeyConstraint baseColumnNames="litige" baseTableName="case"
			constraintName="fk_case_litige_id" referencedColumnNames="id" referencedTableName="litige" />
			<addForeignKeyConstraint baseColumnNames="agent" baseTableName="case"
			constraintName="fk_case_userdb_id" referencedColumnNames="id" referencedTableName="litige" />

	</changeSet>
	<changeSet author="Guillaume (generated)" id="case-3">
		<addPrimaryKey columnNames="id" constraintName="case_pkey" tableName="case" />
	</changeSet>


</databaseChangeLog>